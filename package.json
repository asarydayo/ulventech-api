    "tslint": "^6.1.2",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@sindresorhus/slugify": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express-serve-static-core": "^4.17.8",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
